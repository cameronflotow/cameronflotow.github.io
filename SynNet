            ArrayList<Node> N = new ArrayList<Node>();
            ArrayList<Word> W = new ArrayList<Word>();
            ArrayList<String> S = new ArrayList<String>(); // used words
            ArrayList<String> ShowOutputs = new ArrayList<String>();
            int outX; int outY;
            int rando;
            String textMatch = "";
          
            int cursorShow = 1;
            int cursorTimer = 0;


            int x; int y;
            ArrayList<String> SearchTerm0 = new ArrayList<String>();
            String ST0 = "";
            ArrayList<String> SearchTerm1 = new ArrayList<String>();
            String ST1 = "";
            void setup(){
              background(255);
              //SearchTerm0.add("");
              //SearchTerm1.add("");

              //create network
              for(int i = 0; i < www.length; i++){

                 if(i>0&&i<www.length-2 && www[i] != "."&& www[i] != "a"&& www[i] != "the"){
                   Word w = new Word(www[i]); W.add(w);
                 }else if(www[i] != "."&& www[i] != "a"&& www[i] != "the"){
                   Word w = new Word(www[i]); W.add(w);
                 }
             
               }

              size(800,450);
              outX = width/2;
              outY = (height/2)+(height/18);
              x = 80;
              y = (height/2)-(height/18);



              imageMode(CENTER);
              for(int i = 0; i < 40; i++){
                Node n = new Node(random(width),random(height),1);
                N.add(n);
              }

              for(int i = 0; i < W.size(); i++){
                if(i>0&&i<W.size()-2&&W.get(i).wword != "."&&W.get(i).wword != ","&&W.get(i).wword != "is"){
                  if(W.get(i-1).wword != "." && W.get(i-1).wword != ","&&W.get(i).wword != "is"){W.get(i).Related.add(W.get(i-1).wword);}
                  if(W.get(i+1).wword != "." && W.get(i+1).wword != ","&&W.get(i).wword != "is"){W.get(i).Related.add(W.get(i+1).wword);}
                }
              }

             


                for(int i = 0; i < W.size(); i++){
                  for(int j = 0; j < W.size(); j++){
                    if(i!=j){
                      if(W.get(i).wword.equals(W.get(j).wword)){
                        for(int k = 0; k < W.get(j).Related.size(); k++){
                          W.get(i).Related.add(W.get(j).Related.get(k));
                        }
                     
                        W.remove(j);
                        j = 0;//i=0;
                      }
                    }
                  }
                }
              strokeWeight(1);
              textAlign(CENTER,CENTER);

              for(int i = 0; i < W.size(); i++){
                W.get(i).computeMatch();
              }
                
                matches(51,133);
                
                textSize(15);
            }


            void draw(){
              background(0);
              textAlign(CENTER,CENTER);
              outX = 80; outY = (height/2)+(height/18);
               x = (width/2)-(width/3);
              tint(255);
              cursorTimer++; if(cursorTimer>50){cursorTimer=0;cursorShow*=-1;}
              //image(back,width/2,height/2);
              fill(0,0,255);
              stroke(0,150,255);
              for(int i = 0; i < N.size(); i++){N.get(i).be();}
              for(int i = 0; i < N.size(); i++){ellipse(N.get(i).x,N.get(i).y,6,6);}
              fill(255,255,255,70);
              rect(-1,-1,width+2,height+2);
              fill(255);
              textSize(67);
              text("SynNet",(width/2)-(width/4),(height/2)-(height/5)); 
              //image(tb,(width/2),(height/2)-(height/20));
              fill(255);
              textSize(22);
              for(int i = 0; i < SearchTerm0.size(); i++){text(SearchTerm0.get(i),x,y);text(SearchTerm0.get(i),x,y);text(SearchTerm0.get(i),x,y); x+=15;}
              if(cursorShow == 1){text("|",x,y);text("|",x,y);}


              //image(mag,(width/2)+(width/4)+(width/15),(height/2)+(height/20));
              tint(255);
              //image(plus,(width/2)+(width/4)-(width/30)+(width/28),(height/2)+(height/20));
              textAlign(LEFT,CENTER);
              textSize(11);
              if(ShowOutputs.size()>0){
                for(int i = 0; i < ShowOutputs.size(); i++){
                  text(ShowOutputs.get(i),outX,outY+(height/20)+(height/20));text(ShowOutputs.get(i),outX,outY+(height/20)+(height/20));text(ShowOutputs.get(i),outX,outY+(height/20)+(height/20));
                  outX+=80;
                  if(outX>width-60){
                    outX = 60;
                    outY += 45;
                  }
                }
              }


            }

            class Word{

              String wword;
              String closestMatch = "";
              ArrayList<String> Related = new ArrayList<String>();


              public Word(String wword){
                this.wword=wword;
              } 

              void computeMatch(){
                int matchNum = 0;
                int matchNumTemp = 0;
                String match = "";
                String matchTemp = "";
                for(int i = 0; i < Related.size(); i++){
                  matchTemp = Related.get(i);
                  for(int j = 0; j < Related.size(); j++){
                    if(i!=j){
                      matchTemp.equals(Related.get(j));
                      matchNumTemp++;
                      if(matchNumTemp>matchNum){
                        matchNum = matchNumTemp;
                        matchNumTemp = 0;
                        match = Related.get(i);
                      }
                    }
                  }  
                }
                closestMatch = match;
                Related.add(0,closestMatch);

                for(int i = 0; i < Related.size(); i++){
                  matchTemp = Related.get(i);
                  for(int j = 0; j < Related.size(); j++){
                    if(i!=j){          
                      if(matchTemp.equals(Related.get(j))){
                        Related.remove(j);
                        i=0; j=0;
                      }
                    }
                  }  
                }
                for(int i = 0; i < Related.size(); i++){
                  if(Related.get(i).equals(",")){Related.remove(i); i = 0;}
                }
              }




            }



            class Node{
              int I;
              float x; float y;
              float xi = random(-1,1); float yi = random(-1,1);
              public Node(float x, float y, int I){
                this.x=x; this.y=y; this.I=I;
              } 
              void be(){
                x+=xi; y+=yi;
                if(x<0 || x > width){xi*=-1;}
                if(y<0 || y > height){yi*=-1;}
                line(x,y,N.get(I).x,N.get(I).y);
                xi+=random(-.001,.001);yi+=random(-.001,.001);
                if(xi<-5){xi=-5;} if(xi>5){xi=5;} if(yi<-5){yi=-5;} if(yi>5){yi=5;}
              }
            }




            void matches(int a, int b){
              println("Input 1: "+W.get(a).wword+"  \nInput 2: "+W.get(b).wword);
              for(int i = 0; i < W.get(a).Related.size(); i++){
                for(int j = 0; j < W.get(b).Related.size(); j++){
                  if(W.get(a).Related.get(i) == W.get(b).Related.get(j)){
                    print("   "+W.get(a).Related.get(i));
                  }
                }
              }
            }



            void keyPressed(){

               if(key == BACKSPACE && SearchTerm0.size()>0){
                SearchTerm0.clear();
                ST0="";
                println("");
              }

                if(x < (width/2)+(width/3)){
                  if(key == 'q'){
                    SearchTerm0.add("q");
                  }else if(key == 'w'){
                    SearchTerm0.add("w");
                  }else if(key == 'e'){
                    SearchTerm0.add("e");
                  }else if(key == 'r'){
                    SearchTerm0.add("r");
                  }else if(key == 't'){
                    SearchTerm0.add("t");
                  }else if(key == 'y'){
                    SearchTerm0.add("y");
                  }else if(key == 'u'){
                    SearchTerm0.add("u");
                  }else if(key == 'i'){
                    SearchTerm0.add("i");
                  }else if(key == 'o'){
                    SearchTerm0.add("o");
                  }else if(key == 'p'){
                    SearchTerm0.add("p");
                  }else if(key == 'a'){
                    SearchTerm0.add("a");
                  }else if(key == 's'){
                    SearchTerm0.add("s");
                  }else if(key == 'd'){
                    SearchTerm0.add("d");
                  }else if(key == 'f'){
                    SearchTerm0.add("f");
                  }else if(key == 'g'){
                    SearchTerm0.add("g");
                  }else if(key == 'h'){
                    SearchTerm0.add("h");
                  }else if(key == 'j'){
                    SearchTerm0.add("j");
                  }else if(key == 'k'){
                    SearchTerm0.add("k");
                  }else if(key == 'l'){
                    SearchTerm0.add("l");
                  }else if(key == 'z'){
                    SearchTerm0.add("z");
                  }else if(key == 'x'){
                    SearchTerm0.add("x");
                  }else if(key == 'c'){
                    SearchTerm0.add("c");
                  }else if(key == 'v'){
                    SearchTerm0.add("v");
                  }else if(key == 'b'){
                    SearchTerm0.add("b");
                  }else if(key == 'n'){
                    SearchTerm0.add("n");
                  }else if(key == 'm'){
                    SearchTerm0.add("m");
                  }else if(key == ','){
                    SearchTerm0.add(",");
                }
              }

              if(key==ENTER){
                if(ShowOutputs.size()>0){
                  ShowOutputs.clear();
                }
                for(int i = 0; i < SearchTerm0.size(); i++){
                  ST0+=SearchTerm0.get(i);
                }
                print("Input: "+ST0+" - "); println();
                println();
                for(int i = 0; i < W.size(); i++){
                  if(ST0.equals(W.get(i).wword)){
                    for(int j = 0; j < W.get(i).Related.size(); j++){
                      print(" "+j+") "+W.get(i).Related.get(j));
                      ShowOutputs.add(""+W.get(i).Related.get(j));
                    }
                  }
                }
              }


            }
    
    
    
    
    
    
    
    
    
